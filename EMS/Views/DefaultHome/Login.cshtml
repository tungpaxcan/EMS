@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="utf-8">
    <link rel="manifest" href="https://scanapp.org/assets/manifest.json" />

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script type="text/javascript" src="https://gc.kis.v2.scr.kaspersky-labs.com/FD126C42-EBFA-4E12-B309-BB3FDD723AC1/main.js?attr=hE422l856DXg8CY59UvmLDc-V2GhNMFtFoLCW89uHhBgypldgE59ewZhrCFL6C9l" charset="UTF-8"></script>
